{
  "model": {
    "base_model_id": "runwayml/stable-diffusion-v1-5",
    "condition_type": "canny",
    "conditioning_scale": 1.0
  },
  "training": {
    "learning_rate": 1e-4,
    "batch_size": 32,
    "num_epochs": 80,
    "mixed_precision": true,
    "gradient_accumulation_steps": 1,
    "max_grad_norm": 1.0,
    "prompt_dropout_rate": 0.5,
    "warmup_steps": 500,
    "compile_model": true
  },
  "data": {
    "image_size": 512,
    "max_samples": 10000,
    "augmentation": true,
    "num_workers": 8,
    "pin_memory": true,
    "persistent_workers": true,
    "prefetch_factor": 4
  },
  "logging": {
    "log_every": 50,
    "save_every": 1000,
    "use_wandb": true,
    "project_name": "controlnet-rtx5090"
  },
  "optimizations": {
    "torch_compile": true,
    "cudnn_benchmark": true,
    "channels_last": true,
    "fused_adamw": true,
    "tf32": true
  }
}